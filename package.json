{
  "name": "smartfrigo",
  "version": "2.0.0",
  "scripts": {
    "start": "npm run copyscripts tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "lite": "lite-server",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "zipit": "npm run build && node packagesScripts/zipscript.js",
    "build": "npm run tsc && npm run copyscripts && npm run cr_bundle && npm run cr_bundle_back",
    "cr_bundle": "browserify app/main.js | uglifyjs --screw-ie8 --compress --mangle  > scripts_npm/bundle.js",
    "cr_bundle_back": "browserify back/background.js | uglifyjs --screw-ie8 --compress --mangle > back/bundle.js",
    "copyscripts": "node packagesScripts/copynpmscripts.js"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/common": "^2.1.0",
    "@angular/compiler": "^2.1.0",
    "@angular/core": "^2.1.0",
    "@angular/forms": "^2.1.0",
    "@angular/http": "^2.1.0",
    "@angular/platform-browser": "^2.1.0",
    "@angular/platform-browser-dynamic": "^2.1.0",
    "@angular/router": "^3.4.0",
    "@angular/upgrade": "^2.1.0",
    "angular-in-memory-web-api": "~0.1.5",
    "archiver": "^1.0.0",
    "big-integer": "^1.6.15",
    "bootstrap": "^3.3.6",
    "copy-dir": "^0.3.0",
    "copy-files": "^0.1.0",
    "core-js": "^2.4.0",
    "easy-zip": "0.0.4",
    "jquery": "2.2.4",
    "jquery-ui": "1.10.4",
    "js-zxing-pdf417": "git+https://github.com/PeculiarVentures/js-zxing-pdf417.git",
    "jszip": "latest",
    "pdfjs-dist": "^1.5.434",
    "pdfjs-dist-for-node": "^1.0.892-rc.1",
    "reflect-metadata": "^0.1.3",
    "rxjs": "^5.0.1",
    "systemjs": "0.19.27",
    "uglify-js2": "^2.1.11",
    "zone.js": "^0.6.12",
    "zxing": "^0.1.1"
  },
  "devDependencies": {
    "browserify": "latest",
    "browserify-css": "^0.9.1",
    "browserify-shim": "^3.8.12",
    "concurrently": "^2.0.0",
    "lite-server": "^2.2.0",
    "retyped-archiver-tsd-ambient": "^0.15.0-0"
  },
  "browser": {
    "customPlugin": "path/to/custom/plugin",
    "jquery": "./node_modules/jquery/dist/jquery.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "angular2": "global:angular2",
    "jquery": "jQuery",
    "customPlugin": {
      "depends": [
        "jquery:jQuery"
      ]
    }
  },
  "browserify-css": {
    "autoInject": true,
    "minify": true,
    "rootDir": "."
  },
  "jspm": {
    "registry": "npm",
    "dependencies": {
      "@angular/common": "^2.1.0",
      "@angular/compiler": "^2.1.0",
      "@angular/core": "^2.1.0",
      "@angular/forms": "^2.1.0",
      "@angular/http": "^2.1.0",
      "@angular/platform-browser": "^2.1.0",
      "@angular/platform-browser-dynamic": "^2.1.0",
      "@angular/router": "^3.4.0",
      "@angular/upgrade": "^2.1.0",
      "angular-in-memory-web-api": "^0.1.16",
      "reflect-metadata": "^0.1.8",
      "rxjs": "^5.0.1",
      "zone.js": "^0.7.2"
    },
    "devDependencies": {
      "typescript": "^2.0.7"
    }
  }
}
